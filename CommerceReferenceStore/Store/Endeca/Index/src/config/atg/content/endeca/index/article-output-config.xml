<item item-descriptor-name="article" is-document="true">
  <title property-name="name"/>
  <properties>
    <property name="startDate"  default-value="0" type="date" output-name="content.startDate"/>
    <property name="endDate"  default-value="32506394714894" type="date"  output-name="content.endDate"/>
    <property name="$repositoryId" is-dimension="false" type="string"/>
    <property name="$repositoryId" text-searchable="true" is-rollup-key="true" output-name="content.repositoryId"/>
    <property name="$siteId" is-dimension="true" type="string" output-name="content.siteId"/>
    
  </properties>

</item>

